{
  "$schema": "https://schema.tauri.app/config_2",
  "identifier": "com.radial.builder",
  "productName": "Radial Menu Builder",
  "version": "0.1.0",
  "build": {
    "frontendDist": "../dist"
  },
  "app": {
    "security": {
      "capabilities": ["radial"]
    },
    "windows": [
      {
        "title": "Radial Menu Builder",
        "width": 1200,
        "height": 800,
        "resizable": true
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "windows": {
      "digestAlgorithm": "sha256"
    },
    "macOS": {
      "signingIdentity": null,
      "entitlements": "app.entitlements"
    }
  },
  "plugins": {}
}